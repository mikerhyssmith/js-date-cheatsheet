{
    "basics": [
        {
            "title": "now",
            "code": "DateTime.local();"
        },
        {
            "title": "string",
            "code": "DateTime.fromISO('2016-05-25');",
            "info": "Does not supply a generic parse function but a function for each format"
        },
        {
            "title": "format",
            "code": "DateTime.fromFormat('2018-02-4', 'yyyy-MM-d');",
