            "title": "UTC",
            "code": "DateTime.utc()"
        }
    ],

    "modification": [
        {
            "title": "add",
            "code": ["dateTime.plus({seconds: 10})", "dateTime.plus({minutes: 14})", "dateTime.plus({hours: 24})", "dateTime.plus({days: 31})"]
        },
        {
            "title": "subtract",
            "code": ["dateTime.minus({seconds: 10})", "dateTime.minus({minutes: 14})", "dateTime.minus({hours: 24})", "dateTime.minus({days: 31})"]
        },
        {
            "title": "change",
            "code": ["dateTime.set({seconds: 10})", "dateTime.set({hour: 14})", "dateTime.set({day: 24})", "dateTime.set({weekday: 1})"]
