{
    "basic": [
        {
            "title": "now",
            "code": "DateTime.local();"
        },
        {
            "title": "string",
            "code": ["DateTime.fromISO('2016-05-25');",  "DateTime.fromHTTP('Sunday, 06-Nov-94 08:49:37 GMT');"],
            "info": "Does not supply a generic parse function but a function for each format"
        },
        {
            "title": "format",
            "code": "DateTime.fromFormat('2018-February-4', 'yyyy-MMMM-d');",
